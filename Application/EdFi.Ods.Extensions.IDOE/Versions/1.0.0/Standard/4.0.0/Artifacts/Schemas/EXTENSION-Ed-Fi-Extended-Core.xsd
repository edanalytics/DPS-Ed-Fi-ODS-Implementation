<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://ed-fi.org/4.0.0" xmlns:ann="http://ed-fi.org/annotation" targetNamespace="http://ed-fi.org/4.0.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:include schemaLocation="Ed-Fi-Core.xsd" />
  <xs:annotation>
    <xs:documentation>===== Ed-Fi 4.0.0 Extensions =====</xs:documentation>
  </xs:annotation>
  <xs:annotation>
    <xs:documentation>===== Domain Entities =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-AssessmentAccommodation">
    <xs:annotation>
      <xs:documentation>This entity represents the variations used in how an assessment is presented or taken.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="AssessmentReference" type="AssessmentReferenceType">
            <xs:annotation>
              <xs:documentation>The assessment for which the accommodations are available.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AcademicSubject" type="AcademicSubjectDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>This descriptor holds the description of the content or subject area (e.g., arts, mathematics, reading, stenography, or a foreign language).</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>AcademicSubjectDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="Accommodation" type="AccommodationDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>This descriptor defines variations used in how an assessment is presented or taken.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>AccommodationDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-CalendarDateExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="CalendarDate">
        <xs:sequence>
          <xs:element name="EventMinutes" type="EXTENSION-EventMinutes" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The amount of time for the event in minutes as recognized by the school.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="EventDuration" type="EventDuration" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The amount of time for the event as recognized by the school: 1 day = 1, 1/2 day = 0.5, 1/3 day = 0.33.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-CalendarExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="Calendar">
        <xs:sequence>
          <xs:element name="InstructionalTimeInMinutesIndicator" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>Indicator of whether or not a school is calculating instructional time in minutes instead of days.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-DisciplineActionExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DisciplineAction">
        <xs:sequence>
          <xs:element name="InterimAlternativeEducationSetting" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicator to denote if the special education student was referred to an interim alternative education setting.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DisciplineActionService" type="EXTENSION-DisciplineActionServiceDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The services provided for the student during a discipline action.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-DisciplineActionServiceDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="StudentArrest" type="EXTENSION-StudentArrestDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The arrest type for a discipline action.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-StudentArrestDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="ArrestReason" type="EXTENSION-ArrestReasonDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The arrest reason codes for a discipline action.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-ArrestReasonDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-EducationOrganizationOtherPersonnel">
    <xs:annotation>
      <xs:documentation>Noncertified and other personnel employed by the School Corporation, organized by category (e.g., Office, Clerical, Cafeteria, Maintenance).</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="LocalEducationAgencyReference" type="LocalEducationAgencyReferenceType">
            <xs:annotation>
              <xs:documentation>Reference to the LEA that employees the personnel.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="OtherPersonnelCategory" type="EXTENSION-OtherPersonnelCategoryDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>Category for noncertified and other personnel.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-OtherPersonnelCategoryDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="OtherPersonnelTitle" type="EXTENSION-OtherPersonnelTitleDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>Position title for noncertified and other personnel.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-OtherPersonnelTitleDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="OtherPersonnelCount" type="EXTENSION-OtherPersonnelCount" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Total number of noncertified and other personnel employed by the school corporation by position title.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FullTimeEquivalency" type="FullTimeEquivalency" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The ratio between the hours of work expected in a position and the hours of work normally expected in a full-time position in the same setting.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-SchoolExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="School">
        <xs:sequence>
          <xs:element name="ChoiceIndicator" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicator of whether or not the school is a Choice School.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Accreditation" type="EXTENSION-AccreditationDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>School accreditation (e.g. Accreditted, Not State Accreditted, Recognized Program).</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-AccreditationDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="FreewayBeginDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Freeway accreditation begin date.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FreewayEndDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Freeway accreditation end date.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentEducationOrganizationAssessmentAccommodation">
