  <xs:annotation>
    <xs:documentation>===== Associations =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-StaffEducationOrganizationEmploymentAssociationExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Association</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="StaffEducationOrganizationEmploymentAssociation">
        <xs:sequence>
          <xs:element name="Contract" type="EXTENSION-Contract" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Contract information for the staff member with the education organization (e.g. Contract Days, Contract Salary).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="YearsOfPriorProfessionalExperience" type="NumberOfYears" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The total number of years that an individual has previously held a similar professional position in one or more education institutions.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentAlternativeEducationProgramAssociation">
    <xs:annotation>
      <xs:documentation>Additional information on a student enrolled in an Alternative Education Program.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Association</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="GeneralStudentProgramAssociation">
        <xs:sequence>
          <xs:element name="AlternativeEducationEligibilityReason" type="EXTENSION-AlternativeEducationEligibilityReasonDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>The primary reason for eligibiliy in the alternative education program.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-AlternativeEducationEligibilityReasonDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="ProgramMeetingTime" type="EXTENSION-ProgramMeetingTimeDescriptorReferenceType" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>The time of day the student attends the alternative education program.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-ProgramMeetingTimeDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentCurricularMaterialProgramAssociation">
    <xs:annotation>
      <xs:documentation>Additional information on a student enrolled in an Curricular Material Program.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Association</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="GeneralStudentProgramAssociation">
        <xs:sequence>
          <xs:element name="LivesWithParentReference" type="ParentReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The Parent Unique Identifier that the student is living with for Curricular Material Reimbursement program types.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StudentDoesNotLiveWithParent" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>Flag to indicate that student does not live with parent.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentDisciplineIncidentBehaviorAssociationExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Association</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="StudentDisciplineIncidentBehaviorAssociation">
        <xs:sequence>
          <xs:element name="CriminalGangRelatedEvent" type="EXTENSION-CriminalGangRelatedEventDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The criminal gang related participation of a student in a discipline incident.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-CriminalGangRelatedEventDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentEducationOrganizationAssociationExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Association</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="StudentEducationOrganizationAssociation">
        <xs:sequence>
          <xs:element name="ELLInstrumentUsed" type="EXTENSION-ELLInstrumentUsedDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The instrument used to measure the English proficiency level.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-ELLInstrumentUsedDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="USInitialSchoolEntry" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>US initial school entry date</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentEducationOrganizationResponsibilityAssociationExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Association</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="StudentEducationOrganizationResponsibilityAssociation">
        <xs:sequence>
          <xs:element name="ADMCode" type="EXTENSION-ADMCodeDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Student is enrolled and attending this reported school as a Resident Enrollment,Transfers Out, Cash Transfers, State Obligations, Placements In, or Dual Enrollment.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-ADMCodeDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="InstructionalDays" type="EXTENSION-InstructionalDays" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The number of instructional days for the student at the education organization for a given school year. Allowable values are 1 - 180.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="InstructionalMinutes" type="EXTENSION-InstructionalMinutes" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The number of instructional days for the student at the education organization during the course of one full day. Allowable values are 1 - 400.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AdditionalEdOrgResponsibility" type="EXTENSION-AdditionalEdOrgResponsibility" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Additional education organization responsibility designation for the student.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentSpecialEducationProgramAssociationExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Association</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="StudentSpecialEducationProgramAssociation">
        <xs:sequence>
          <xs:element name="CircumstancesRelevantToTimeline" type="EXTENSION-CircumstancesRelevantToTimelineDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The circumstance relevant to the evaluation timeline.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-CircumstancesRelevantToTimelineDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="TimelineCompliance" type="EXTENSION-TimelineComplianceDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Reason the special education deadline was missed or services did not begin.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-TimelineComplianceDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="ParentalConsentDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The date used to consider the evaluation timeline? Allowable formats are MMDDYYYY and MM/DD/YYYY. Date must be instructional day in your school calendar. If referral occurred in summer, parent constent / referal date should be first day of current school year.</xs:documentation>
            </xs:annotation>
          </xs:element>
